<?xml version='1.0'?>
<!DOCTYPE policy PUBLIC
"-//JBoss//DTD JBOSS Security Config 3.0//EN"
"http://www.jboss.org/j2ee/dtd/security_config.dtd">

<policy>
    <application-policy name="firebird">
        <authentication>
            <login-module code="com.fiveamsolutions.nci.commons.authentication.SessionFixationProtectionLoginModule" flag="required"/>
            <login-module code="gov.nih.nci.firebird.security.FirebirdAuthenticationLoginModule" flag="required"/>
        </authentication>
    </application-policy>
</policy>
